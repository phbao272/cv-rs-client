    "@mui/material": "^5.10.3",
    "@mui/x-date-pickers": "^6.0.3",
    "@tanstack/react-query": "^4.26.1",
    "@tanstack/react-table": "^8.8.4",
    "axios": "^0.27.2",
    "date-fns": "^2.29.3",
    "jotai": "^2.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.43.5",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^9.1.2",
    "styled-components": "^5.3.9",
    "zod": "^3.21.4"
